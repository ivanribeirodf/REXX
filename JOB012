/*REXX*/
 SAY '----------------------'
 SAY '- PROGRAMA DE MEDIA  -'
 SAY 'TECLE ENTER'

NEWSTACK
  SAY QUEUED()  /* INFORMAR A QUANTIDADE DE REGS NA FILA */
  QUEUE 'DADOS DA LINHA1'
  QUEUE 'DADOS DA LINHA2'
  QUEUE 'DADOS DA LINHA3'
  QUEUE 'DADOS DA LINHA4'
  QUEUE 'DADOS DA LINHA5'
  SAY QUEUED()  /* INFORMAR A QUANTIDADE DE REGS NA FILA */
  DO I = 1 TO QUEUED()
    PULL LINHAPILHA
    SAY LINHAPILHA
  END
DELSTACK
NEWSTACK
  SAY QUEUED()  /* INFORMAR A QUANTIDADE DE REGS NA FILA */
  PUSH 'DADOS DA LINHA1'
  PUSH 'DADOS DA LINHA2'
  PUSH 'DADOS DA LINHA3'
  PUSH 'DADOS DA LINHA4'
  PUSH 'DADOS DA LINHA5'
  SAY QUEUED()  /* INFORMAR A QUANTIDADE DE REGS NA FILA */
  DO I = 1 TO QUEUED()
    PULL LINHAPILHA
    SAY LINHAPILHA
  END
DELSTACK
